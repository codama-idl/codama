{% extends "layout.njk" %}

{% block main %}

use solana_pubkey::Pubkey;

{% for program in programsToExport | sort(false, false, 'name') %}

  /// `{{ program.name | snakeCase }}` program ID.
  pub const {{ program.name | snakeCase | upper }}_ID: Pubkey = Pubkey::from_str_const("{{ program.publicKey }}");
{% endfor %}

{% endblock %}
