{% extends "layout.njk" %}

{% block main %}
{% for definedType in definedTypesToExport | sort(false, false, 'name') %}
from . import {{ definedType.name | camelCase }}
{% if definedType.type.kind == "enumTypeNode" %}
from .{{ definedType.name | camelCase }} import {{ definedType.name | pascalCase }}JSON,{{ definedType.name | pascalCase }}Kind;
{% elseif definedType.type.kind == "structTypeNode"  %}
from .{{ definedType.name | camelCase }} import {{ definedType.name | pascalCase }}JSON,{{ definedType.name | pascalCase }};
{% endif %}
{% else %}
{% endfor %}
{% endblock %}
